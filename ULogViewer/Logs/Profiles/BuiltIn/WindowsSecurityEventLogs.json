{
	"DataSource": {
		"Name": "WindowsEventLogs",
		"Options": {
			"Category": "Security"
		}
	},
	"Icon": "Windows",
	"IsAdministratorNeeded": true,
	"LogLevelMapForReading": {
		"f": "Failure",
		"s": "Success"
	},
	"LogLevelMapForWriting": {
		"Failure": "F",
		"Success": "S",
		"Undefined": "U"
	},
	"LogPatterns": [
		{
			"Regex": "^\\<Timestamp\\>(?<Timestamp>[^\\<]+)\\</Timestamp\\>"
		},
		{
			"Regex": "^\\<EventId\\>(?<Event>[^\\<]*)\\</EventId\\>"
		},
		{
			"Regex": "^\\<Level\\>(?<Level>[^\\<]+)\\</Level\\>"
		},
		{
			"Regex": "^\\<Source\\>(?<SourceName>[^\\<]*)\\</Source\\>"
		},
		{
			"Regex": "^\\<Message\\>"
		},
		{
			"Regex": "^(?<Message>($|[^\\<]+))",
			"IsRepeatable": true,
			"IsSkippable": true
		},
		{
			"Regex": "^\\</Message\\>"
		}
	],
	"LogStringEncodingForReading": "Xml",
	"LogWritingFormat": "[{Level}] {Timestamp} {Event,-5} {SourceName}:{NewLine}{Message}{NewLine}",
	"SortDirection": "Descending",
	"TimestampFormatForDisplaying": "yyyy/MM/dd HH:mm:ss",
	"TimestampFormatsForReading": [
		"yyyy/MM/dd HH:mm:ss"
	],
	"VisibleLogProperties": [
		{
			"Name": "Timestamp",
			"Width": 180
		},
		{
			"Name": "SourceName",
			"Width": 200
		},
		{
			"Name": "Event",
			"Width": 80
		},
		{
			"Name": "Message"
		}
	]
}