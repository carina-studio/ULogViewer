{
	"DataSource": {
		"Name": "StandardOutput",
		"Command": "git --no-pager log"
	},
	"Icon": "VersionControl",
	"IsWorkingDirectoryNeeded": true,
	"LogPatterns": [
		{
			"Regex": "^commit[\\s]+.+$"
		},
		{
			"Regex": "^Author:[\\s]+(?<UserName>.+)$"
		},
		{
			"Regex": "^Date:[\\s]+[\\w]+[\\s]+(?<Timestamp>[\\w]+[\\s]+[\\d]+[\\s]+[\\d]+:[\\d]+:[\\d]+[\\s]+[\\d]+).*$"
		},
		{
			"Regex": "^[\\s]*$"
		},
		{
			"IsRepeatable": true,
			"IsSkippable": true,
			"Regex": "^[\\s]{4}(?<Message>.*)$"
		},
		{
			"Regex": "^[\\s]*$"
		}
	],
	"LogWritingFormat": "Author: {UserName}{NewLine}Date:   {Timestamp}{NewLine}{NewLine}{Message}{NewLine}",
	"SortDirection": "Descending",
	"TimestampFormatForDisplaying": "yyyy/MM/dd HH:mm:ss",
	"TimestampFormatForReading": "MMM d HH:mm:ss yyyy",
	"VisibleLogProperties": [
		{
			"Name": "Timestamp",
			"Width": 180
		},
		{
			"DisplayName": "Author",
			"Name": "UserName",
			"Width": 250
		},
		{
			"Name": "Message"
		}
	]
}