{
	"DataSource": {
		"Name": "StandardOutput",
		"Options": {
			"Command": "git --no-pager log"
		}
	},
	"Icon": "VersionControl",
	"IsWorkingDirectoryNeeded": true,
	"LogPatterns": [
		{
			"Regex": "^commit[\\s]+(?<Extra1>[\\d\\w]+).*$"
		},
		{
			"Regex": "^Merge:.*$",
			"IsSkippable": true
		},
		{
			"Regex": "^Author:[\\s]+(?<UserName>.+)$"
		},
		{
			"Regex": "^Date:[\\s]+[\\w]+[\\s]+(?<Timestamp>[\\w]+[\\s]+[\\d]+[\\s]+[\\d]+:[\\d]+:[\\d]+[\\s]+[\\d]+).*$"
		},
		{
			"Regex": "^[\\s]*$"
		},
		{
			"IsRepeatable": true,
			"IsSkippable": true,
			"Regex": "^[\\s]{4}(?<Message>.*)$"
		},
		{
			"Regex": "^[\\s]*$"
		}
	],
	"LogWritingFormats": [
		"commit: {Extra1}{NewLine}Author: {UserName}{NewLine}Date:   {Timestamp}{NewLine}{NewLine}{Message}{NewLine}"
	],
	"SortDirection": "Descending",
	"TimestampCategoryGranularity": "Day",
	"TimestampFormatForDisplaying": "yyyy/MM/dd HH:mm:ss",
	"TimestampFormatsForReading": [
		"MMM d HH:mm:ss yyyy"
	],
	"VisibleLogProperties": [
		{
			"Name": "Timestamp",
			"Width": 180
		},
		{
			"DisplayName": "Commit",
			"Name": "Extra1",
			"Width": 150
		},
		{
			"DisplayName": "Author",
			"Name": "UserName",
			"Width": 150
		},
		{
			"Name": "Message"
		}
	]
}