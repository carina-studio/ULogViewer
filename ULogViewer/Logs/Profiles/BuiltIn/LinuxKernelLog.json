{
	"DataSource": {
		"Name": "File",
		"Options": {
			"FileName": "/var/log/kern.log"
		}
	},
	"Icon": "Linux",
	"LogPatterns": [
		{
			"Regex": "^(?<Timestamp>[\\w]+[\\s]+[\\d]+[\\s]+[\\d\\:\\.]+)[\\s]+(?<DeviceName>[^\\s]*)[\\s]+(?<SourceName>[^\\[\\:]*)\\:[\\s](\\[[\\s]*(?<Extra1>[\\d\\.]+)[\\s]*\\][\\s]?)?(?<Message>.*)$"
		}
	],
	"LogWritingFormat": "{Timestamp} {DeviceName} {SourceName}: [{Extra1,12}] {Message}",
	"SortKey": "Timestamp",
	"TimestampFormatForDisplaying": "MM/dd HH:mm:ss",
	"TimestampFormatsForReading": [
		"MMM dd HH:mm:ss",
		"MMM  d HH:mm:ss",
		"MMM d HH:mm:ss"
	],
	"TimestampFormatForWriting": "MMM d HH:mm:ss",
	"VisibleLogProperties": [
		{
			"Name": "Timestamp",
			"Width": 130
		},
		{
			"DisplayName": "RelativeTimestamp",
			"Name": "Extra1",
			"Width": 130
		},
		{
			"Name": "DeviceName",
			"Width": 150
		},
		{
			"Name": "SourceName",
			"Width": 100
		},
		{
			"Name": "Message"
		}
	]
}