{
	"DataSource": {
		"Name": "StandardOutput",
		"Command": "adb logcat -v threadtime",
		"SetupCommands": [
			"adb logcat -c"
		]
	},
	"Icon": "Android",
	"IsContinuousReading": true,
	"LogLevelMapForReading": {
		"D": "Debug",
		"E": "Error",
		"F": "Fatal",
		"I": "Info",
		"V": "Verbose",
		"W": "Warn"
	},
	"LogPatterns": [
		{
			"Regex": "^(?<Timestamp>[^\\s]+[\\s]+[^\\s]+)[\\s]+(?<ProcessId>[\\d]+)[\\s]+(?<ThreadId>[\\d]+)[\\s]+(?<Level>[\\w]+)[\\s]+(?<SourceName>[^\\s\\:]*([\\s]+[^\\s\\:]*)*)\\:[\\s](?<Message>.*)$"
		}
	],
	"SortKey": "Id",
	"TimestampCultureInfoForReading": "en-US",
	"TimestampFormatForDisplaying": "MM/dd HH:mm:ss.fff",
	"TimestampFormatForReading": "MM-dd HH:mm:ss.fff",
	"VisibleLogProperties": [
		{
			"Name": "Timestamp",
			"Width": 180
		},
		{
			"Name": "ProcessId",
			"Width": 80
		},
		{
			"Name": "ThreadId",
			"Width": 80
		},
		{
			"DisplayName": "Tag",
			"Name": "SourceName",
			"Width": 100
		},
		{
			"Name": "Message"
		}
	]
}