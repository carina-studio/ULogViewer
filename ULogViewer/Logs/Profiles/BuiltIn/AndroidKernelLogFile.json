	"LogPatterns": [
		{
			"Regex": "^(?<Timestamp>[^\\s]+[\\s]+[^\\s]+)[\\s]+[\\d]+[\\s]+[\\d]+[\\s]+(?<Level>[\\w]+)[\\s]+(\\:)?[\\s]*(\\[(?<Timestamp>[\\d]+_[\\d\\:\\.]+)\\](\\@[\\d]+)?[\\s]*)?((?<SourceName>([^\\s\\:]|([\\s]+[^\\s\\:])|([\\:]+[^\\s\\:]))*[\\:]*[\\s]*)\\:)?[\\s](?<Message>.*)$"
		}
	],
	"LogWritingFormat": "{Timestamp} {Level} {SourceName}: {Message}",
