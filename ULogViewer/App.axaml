<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="CarinaStudio.ULogViewer.App">

	<!-- Resources -->
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="/Styles/Resources.axaml"/>
				<ResourceInclude Source="/Styles/Icons.axaml"/>
				<ResourceInclude Source="/Strings/Default.axaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>

	<!-- Styles -->
	<Application.Styles>
		<StyleInclude Source="/Styles/Default.axaml"/>
	</Application.Styles>
	
</Application>
