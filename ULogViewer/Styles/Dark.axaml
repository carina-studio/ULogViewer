<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<!-- Base style -->
	<FluentTheme Mode="Dark"/>

	<!-- Resources -->
	<Styles.Resources>
		<ResourceDictionary>

			<!-- SessionView -->
			<SolidColorBrush x:Key="Brush.SessionView.LogHeader.Background">#0e0e0e</SolidColorBrush>
			<SolidColorBrush x:Key="Brush.SessionView.LogHeader.Separator">#7fffffff</SolidColorBrush>
			<SolidColorBrush x:Key="Brush.SessionView.LogListBox.Border">#ffffff</SolidColorBrush>

			<!-- Window -->
			<SolidColorBrush x:Key="Brush.Window.Background">#1e1e1e</SolidColorBrush>
			
		</ResourceDictionary>
	</Styles.Resources>
    
</Styles>
