<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<!-- Base style -->
	<StyleInclude Source="/Styles/Default.axaml"/>

	<!-- Resources -->
	<Styles.Resources>
		<ResourceDictionary>

			<!-- Icon -->
			<LinearGradientBrush x:Key="Brush/Icon.LogProfile.Apple" EndPoint="75%,75%" StartPoint="25%,25%">
				<GradientStop Color="#aaaaaa" Offset="0"/>
				<GradientStop Color="#777777" Offset="1"/>
			</LinearGradientBrush>
			<LinearGradientBrush x:Key="Brush/Icon.LogProfile.Book" EndPoint="100%,0%" StartPoint="0%,0%">
				<GradientStop Color="#bf4c00" Offset="0"/>
				<GradientStop Color="#8c3800" Offset="1"/>
			</LinearGradientBrush>
			<LinearGradientBrush x:Key="Brush/Icon.LogProfile.Code" EndPoint="100%,90%" StartPoint="0%,10%">
				<GradientStop Color="#aaaaaa" Offset="0"/>
				<GradientStop Color="#707070" Offset="1"/>
			</LinearGradientBrush>
			<LinearGradientBrush x:Key="Brush/Icon.LogProfile.Experiment" EndPoint="0%,95%" StartPoint="0%,5%">
				<GradientStop Color="#00bf13" Offset="0"/>
				<GradientStop Color="#00730b" Offset="1"/>
			</LinearGradientBrush>
			<LinearGradientBrush x:Key="Brush/Icon.LogProfile.Json" EndPoint="0%,95%" StartPoint="0%,5%">
				<GradientStop Color="#0fa69c" Offset="0"/>
				<GradientStop Color="#096660" Offset="1"/>
			</LinearGradientBrush>
			<LinearGradientBrush x:Key="Brush/Icon.LogProfile.MobileDevices" EndPoint="90%,90%" StartPoint="10%,10%">
				<GradientStop Color="#a0a0a0" Offset="0"/>
				<GradientStop Color="#707070" Offset="1"/>
			</LinearGradientBrush>
			<LinearGradientBrush x:Key="Brush/Icon.LogProfile.Phone" EndPoint="70%,90%" StartPoint="30%,10%">
				<GradientStop Color="#a0a0a0" Offset="0"/>
				<GradientStop Color="#707070" Offset="1"/>
			</LinearGradientBrush>
			<LinearGradientBrush x:Key="Brush/Icon.LogProfile.Statistic" EndPoint="0%,80%" StartPoint="0%,20%">
				<GradientStop Color="#aa73ff" Offset="0"/>
				<GradientStop Color="#762de6" Offset="1"/>
			</LinearGradientBrush>
			<LinearGradientBrush x:Key="Brush/Icon.LogProfile.TabletDevice" EndPoint="100%,90%" StartPoint="0%,10%">
				<GradientStop Color="#a0a0a0" Offset="0"/>
				<GradientStop Color="#707070" Offset="1"/>
			</LinearGradientBrush>
			<LinearGradientBrush x:Key="Brush/Icon.LogProfile.Terminal" EndPoint="90%,100%" StartPoint="10%,0%">
				<GradientStop Color="#a0a0a0" Offset="0"/>
				<GradientStop Color="#707070" Offset="1"/>
			</LinearGradientBrush>

			<!-- LogLevel -->
			<SolidColorBrush x:Key="Brush/ULogViewer.LogLevel.Debug.Background">#80bfff</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/ULogViewer.LogLevel.Debug.Foreground">#4da6ff</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/ULogViewer.LogLevel.Error.Background">#ff6666</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/ULogViewer.LogLevel.Error.Foreground">#ff3232</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/ULogViewer.LogLevel.Failure.Background">#e66767</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/ULogViewer.LogLevel.Failure.Foreground">#e63939</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/ULogViewer.LogLevel.Fatal.Background">#ff4d4d</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/ULogViewer.LogLevel.Fatal.Foreground">#ff0d0d</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/ULogViewer.LogLevel.Info.Background">#94ff66</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/ULogViewer.LogLevel.Info.Foreground">#4cff00</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/ULogViewer.LogLevel.Success.Background">#76cc52</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/ULogViewer.LogLevel.Success.Foreground">#3dcc00</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/ULogViewer.LogLevel.Trace.Background">#66f2ff</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/ULogViewer.LogLevel.Trace.Foreground">#32eeff</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/ULogViewer.LogLevel.Undefined.Background">#cccccc</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/ULogViewer.LogLevel.Undefined.Foreground">#ffffff</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/ULogViewer.LogLevel.Verbose.Background">#cccccc</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/ULogViewer.LogLevel.Verbose.Foreground">#ffffff</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/ULogViewer.LogLevel.Warn.Background">#ffa366</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/ULogViewer.LogLevel.Warn.Foreground">#ff8432</SolidColorBrush>

			<!-- LogProfileSelectionDialog -->
			<SolidColorBrush x:Key="Brush/LogProfileSelectionDialog.Label.Background">#000000</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/LogProfileSelectionDialog.Label.Background.Pro">#422e6B</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/LogProfileSelectionDialog.Label.Border">#444444</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/LogProfileSelectionDialog.Label.Border.Pro">#583d8f</SolidColorBrush>

			<!-- SessionView -->
			<SolidColorBrush x:Key="Brush/SessionView.LogChart.Axis.Separator" Color="#606060"/>
			<SolidColorBrush x:Key="Brush/SessionView.LogHeader.Background">#222222</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/SessionView.LogHeader.Border">#333333</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/SessionView.LogHeader.Separator">#3fffffff</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/SessionView.LogListBox.Item.Background.Marked.Blue">#2f0094ff</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/SessionView.LogListBox.Item.Background.Marked.Default">#2fffffff</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/SessionView.LogListBox.Item.Background.Marked.Green">#2f4fcc00</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/SessionView.LogListBox.Item.Background.Marked.Indigo">#2f0026ff</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/SessionView.LogListBox.Item.Background.Marked.Magenta">#2fff0072</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/SessionView.LogListBox.Item.Background.Marked.Orange">#2fff6a00</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/SessionView.LogListBox.Item.Background.Marked.Purple">#2fb200ff</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/SessionView.LogListBox.Item.Background.Marked.Red">#2fff0000</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/SessionView.LogListBox.Item.Background.Marked.Yellow">#2fffff00</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/SessionView.LogListBox.Item.Background.Pressed" Color="{DynamicResource Color/Accent}" Opacity="0.3"/>
			<SolidColorBrush x:Key="Brush/SessionView.LogListBox.Item.Background.Selected" Color="{DynamicResource Color/Accent}" Opacity="0.4"/>
			<SolidColorBrush x:Key="Brush/SessionView.LogListBox.Item.Background.Selected.PointerOver" Color="{DynamicResource Color/Accent}" Opacity="0.6"/>
			<SolidColorBrush x:Key="Brush/SessionView.LogListBox.Item.Background.SelectedValue" Color="{DynamicResource Color/Accent}" Opacity="0.3"/>
			<SolidColorBrush x:Key="Brush/SessionView.LogListBox.Item.Column.Border.PointerOver">#7fffffff</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/SessionView.LogListBox.Item.SelectionIndicator.Background">#40ffffff</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/SessionView.LogListBox.Item.Separator">#555555</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/SessionView.MarkedLogListBox.Item.Border.Selected">#7fffffff</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/SessionView.StatusBar.Background.Inactive">#606060</SolidColorBrush>
			<SolidColorBrush x:Key="Brush/SessionView.StatusBar.Background.Paused">#606060</SolidColorBrush>
			
		</ResourceDictionary>
	</Styles.Resources>
    
</Styles>
